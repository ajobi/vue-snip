  "scripts": {
    "build": "webpack",
    "prepare": "yarn build",
    "lint": "eslint ./",
    "start": "NODE_ENV=development webpack-dev-server",
    "test:open": "NODE_ENV=test start-server-and-test 'webpack-dev-server' http://localhost:9001 'cypress open'",
