{
  "name": "vue-snip",
  "version": "1.2.0",
  "author": "Andrej Bilec <andrej.bilec@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "yarn build",
    "lint": "eslint ./",
    "build": "rollup -c",
    "dev": "NODE_ENV=dev rollup -c rollup.server.js",
    "dev:demo": "NODE_ENV=demo rollup -c rollup.server.js",
    "test:open": "yarn instrument && yarn build && cypress open",
    "test:run": "yarn instrument && yarn build && cypress run",
    "test": "yarn test:run && yarn coverage",
    "instrument": "nyc instrument src instrumented",
    "coverage": "npx nyc report --reporter=text --reporter=text-summary",
    "coveralls": "npx nyc report --reporter=text-lcov | coveralls"
  },
  "main": "dist/main.js",
  "module": "dist/main.es.js",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.10.3",
    "@cypress/code-coverage": "^3.8.1",
    "@rollup/plugin-babel": "^5.3.0",
    "coveralls": "^3.1.0",
    "cypress": "^9.1.0",
    "esbuild": "^0.13.15",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "rollup": "^2.60.1",
    "rollup-plugin-esbuild": "^4.7.2",
    "rollup-plugin-serve": "^1.1.0"
  },
  "homepage": "https://github.com/ajobi/vue-snip",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajobi/vue-snip"
  },
  "keywords": [
    "clamp",
    "ellipsis",
    "snip",
    "snip-text",
    "cut-text",
    "clamp-text",
    "truncate-text",
    "multiline-text",
    "vue",
    "vue2",
    "vue3",
    "vuejs",
    "vue-directive",
    "vue-plugin"
  ]
}
